{
  "name": "@localisprimary/esi",
  "version": "1.3.0",
  "type": "module",
  "description": "Auto-generated TypeScript client for the EVE Online API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "fetch-schema": "node scripts/fetch-schema.ts",
    "generate": "node scripts/generate.ts && pnpm lint:fix",
    "build": "rimraf dist && tsc",
    "compile": "pnpm fetch-schema && pnpm generate && pnpm build && pnpm test",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run",
    "change": "beachball change"
  },
  "beacball": {
    "access": "public"
  },
  "keywords": [
    "eve",
    "eve online",
    "esi",
    "api",
    "client",
    "typescript"
  ],
  "author": "Adam Trager <hello@adamtrager.com>",
  "funding": "https://buymeacoffee.com/nfinished",
  "repository": {
    "type": "git",
    "url": "https://github.com/localisprimary/esi.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/ui": "^3.2.4",
    "beachball": "^2.55.0",
    "camelcase": "^8.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": "^24.4.1"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
