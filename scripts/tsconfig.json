{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "ES2022",
    "target": "ES2024",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "rootDir": ".",
    "declaration": false,
    "types": ["node"]
  },
  "include": ["./**/*.ts"],
  "exclude": ["./dist", "./openapi.json"]
}
