{
  "openapi": "3.0.0",
  "info": {
    "title": "Test API",
    "version": "2.0.0"
  },
  "paths": {
    "/alliances/{alliance_id}/": {
      "get": {
        "operationId": "get_alliances_alliance_id",
        "description": "Get alliance information",
        "parameters": [
          {
            "name": "alliance_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "required_param",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Alliance information",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AllianceV2"
                }
              }
            }
          }
        }
      }
    },
    "/characters/{character_id}/": {
      "get": {
        "operationId": "get_characters_character_id",
        "description": "Get character information",
        "parameters": [
          {
            "name": "character_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "datasource",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": ["tranquility"]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Character information",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Character"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AllianceV2": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "ticker": {
            "type": "string"
          }
        }
      },
      "Character": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "corporation_id": {
            "type": "integer"
          }
        }
      }
    }
  }
}
